<%= turbo_frame_tag "search_results" do %>
    <% venues.each do |v| %>
           <li class="mt-3">
             <div class="d-flex justify-content-between">
                <div class="d-flex">
                    <% if v.profile_picture.attached? %>
                        <%= image_tag v.profile_picture, class: "img-thumbnail rounded-circle", style: "width: 2.5rem" %>
                    <% else %> 
                        <%= image_tag 'thumbnail.png', class: "img-thumbnail rounded-circle ", style: "width: 2.5rem" %>
                    <%end%>
                    <div class="d-flex flex-column mx-2 justify-content-center">
                        <small class="fw-bold text-black" style="overflow: hidden; text-overflow: ellipsis; white-space: nowrap; max-width:12rem; "><%= link_to v, class: "a-decoration-none", data: {turbo: false} do %><%= v.username %><%end%></small>
                    </div>
                </div>
             </div>    
            </li>
    <% end %>
<%end%>